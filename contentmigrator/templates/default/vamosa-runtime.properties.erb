vamosa.serverLogFolderPath=<%= node["tomcat"]["base"] %>/logs
vamosa.taskLibraryPath=<%= node["contentmigrator"]["tasklib_home"] %>
vamosa.executor.corePoolSize=8
vamosa.executor.maxPoolSize=20
vamosa.executor.idleThreadKeepAlive=120
vamosa.executor.queueSize=20

vamosa.log.pipelineReports.serverHost=localhost
vamosa.log.pipelineReports.serverPort=27017
vamosa.log.pipelineReports.databaseName=pipelineReports

activeMQ.brokerURL=tcp://localhost:61616

# Uncomment and modify if you're using MySQL
hibernate.dialect=org.hibernate.dialect.MySQL5InnoDBDialect
hibernate.connection.driver_class=com.mysql.jdbc.Driver
hibernate.connection.url=jdbc:mysql://<%= node[:contentmigrator][:mysql_host] %>/<%= node[:contentmigrator][:mysql_database] %>
hibernate.connection.username=<%= node[:contentmigrator][:mysql_username] %>
hibernate.connection.password=<%= node[:contentmigrator][:mysql_password] %>

